PSUADE
INPUT
   dimension = 3
   variable   1 X = 0  1
   variable   2 Y = 0  1
   variable   3 U = 0  1
END
OUTPUT
   dimension = 4
   variable 1 Y
   variable 2 D1
   variable 3 D2
   variable 4 D3
END
METHOD
   sampling = MC
   num_samples = 1
   randomize
END
APPLICATION
   driver = ./simulator_deriv
END
ANALYSIS
   analyzer output_id  = 1
   printlevel 1
END
END
